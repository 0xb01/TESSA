#tag ClassProtected Class StartupTimerInherits Timer	#tag Event		Sub Action()		  'AddDebugText_("StartupTimer Action")		  		  if App.MainProgramm<>nil then		    if App.StartupRun then		      //add a testreport to the automated sequence		      TESSAGlobalFunctions.SAMReport=1		      MainWindow.AddSAMReportToTestcase		      App.MainLoop( "startup")		      AddDebugText_("StartupTimer Action")		    end		  end		End Sub	#tag EndEvent	#tag Method, Flags = &h0		Sub Start(InitTime as integer)		  		  Me.Period = InitTime		  Me.Mode = Timer.ModeSingle		  		End Sub	#tag EndMethod	#tag ViewBehavior		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			Type="Integer"			EditorType="Integer"		#tag EndViewProperty		#tag ViewProperty			Name="Mode"			Visible=true			Group="Behavior"			InitialValue="2"			Type="Integer"			EditorType="Enum"			#tag EnumValues				"0 - Off"				"1 - Single"				"2 - Multiple"			#tag EndEnumValues		#tag EndViewProperty		#tag ViewProperty			Name="Name"			Visible=true			Group="ID"			Type="String"			EditorType="String"		#tag EndViewProperty		#tag ViewProperty			Name="Period"			Visible=true			Group="Behavior"			InitialValue="1000"			Type="Integer"		#tag EndViewProperty		#tag ViewProperty			Name="Super"			Visible=true			Group="ID"			Type="String"			EditorType="String"		#tag EndViewProperty	#tag EndViewBehaviorEnd Class#tag EndClass