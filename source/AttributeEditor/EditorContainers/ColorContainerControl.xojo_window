#tag Events RedColor
	#tag Event
		Sub Open()
		  Me.SetType(NumbericValidator.Types.UnsignedIntegerType)
		End Sub
	#tag EndEvent
	#tag Event
		Sub ValidTextChange()
		  Me.Text = Str(SetCorrectColorRange(Val(Me.Text)))
		  UpdateValue
		End Sub
	#tag EndEvent
	#tag Event
		Sub EnterTabPressed()
