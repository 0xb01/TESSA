#tag Events Preview
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  #Pragma Unused X
		  #Pragma Unused Y
		  
		  Dim col as Color = Me.FillColor
		  Dim ok as Boolean = SelectColor(col, "Select custom color")
		  if ok then
		    ColorField.Text = ColorToString(col)
		    UpdateRGBFileds
		  end
		  Return true
		End Function
	#tag EndEvent
#tag EndEvents
