#tag WindowCode
	#tag Method, Flags = &h0
		Function currentValue() As Variant
		  // Part of the AttributeContainerInterface interface.
		  Return ColorToString(Preview.FillColor)
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub InitContainer(Attr as AttributeClass)
		  // Part of the AttributeContainerInterface interface.
		  if Attr isa Color_AttributeClass then
		    mCurrentAttribute = Color_AttributeClass(attr)
		    mInitValue = mCurrentAttribute.GIAS
		    UpdateAttribute
		    Me.Visible = True
		  end
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Function IsEnable() As Boolean
		  // Part of the AttributeContainerInterface interface.
		  Return Me.Enabled
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub SaveValue()
		  // Part of the AttributeContainerInterface interface.
		  if mCurrentAttribute <> Nil And mCurrentAttribute.GIAS <> ColorToString(Preview.FillColor) Then
		    mCurrentAttribute.SIAS(ColorToString(Preview.FillColor))
		    mCurrentAttribute.AttributeChanged
		    ValueSaved
		  End
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Function SetCorrectColorRange(Value as integer) As integer
		  if (Value > 255) then
		    Value = 255
		  ElseIf Value < 0 then
		    Value = 0
		  end
		  return Value
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub setCurrentValue(data As Variant)
		  // Part of the AttributeContainerInterface interface.
		  if currentValue <> data Then
		    if data.Type = Variant.TypeString Then
		      ColorField.Text = data.StringValue
		    ElseIf data.Type = Variant.TypeColor Then
		      ColorField.Text = ColorToString(data.ColorValue)
		    End
		    UpdateRGBFileds
		    mInitValue = ColorField.Text
		    UpdateUndoButton(False)
		  End
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub SetEnable(value As Boolean)
		  // Part of the AttributeContainerInterface interface.
		  Me.Enabled = value
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub UndoValue()
		  // Part of the AttributeContainerInterface interface.
		  ColorField.Text = mInitValue
		  UpdateRGBFileds
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub UpdateAttribute()
		  if mCurrentAttribute <> Nil then
		    ColorField.Text = mCurrentAttribute.GIAS
		    UpdateRGBFileds
		  End
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub UpdateRGBFileds()
		  dim newColor As Color = GetColor(ColorField.Text)
		  RedColor.Text = Str(newColor.Red)
		  GreenColor.Text = Str(newColor.Green)
		  BlueColor.Text = Str(newColor.Blue)
		  UpdateValue
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub UpdateValue()
		  Preview.FillColor = RGB(Val(RedColor.Text), Val(GreenColor.Text), Val(BlueColor.Text))
		  ColorField.Text = ColorToString(Preview.FillColor)
		  UpdateUndoButton(ColorField.Text <> mInitValue)
		End Sub
	#tag EndMethod


	#tag Hook, Flags = &h0
		Event UpdateUndoButton(needToDisplay As Boolean)
	#tag EndHook

	#tag Hook, Flags = &h0
		Event ValueSaved()
	#tag EndHook


	#tag Property, Flags = &h21
		Private mCurrentAttribute As Color_AttributeClass = nil
	#tag EndProperty

	#tag Property, Flags = &h21
		Private mInitValue As String
	#tag EndProperty


